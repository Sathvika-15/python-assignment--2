def strg(strng):
    lower_count = 0
    num_count = 0
    upper_count = 0
    for i in strng:
       if(i.isalpha()):
         if(i.islower()):
               lower_count += 1
         elif(i.isupper()):
               upper_count += 1
       
       elif(i.isnumeric()):
           num_count += 1
    print("lower_count:",lower_count)
    print("num_count:",num_count)
    print("upper_count:",upper_count)

    
strg('SQKdHcq12G45')
